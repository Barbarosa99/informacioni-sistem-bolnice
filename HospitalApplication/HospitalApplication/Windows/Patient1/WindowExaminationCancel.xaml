<Window x:Class="HospitalApplication.WindowExaminationCancel"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HospitalApplication"
        mc:Ignorable="d"
        Title="WindowExaminationCancel" Height="800" Width="500">
    <Grid Margin="10">
        <StackPanel>
            <Label Content="Id of examination:"></Label>
            <TextBox Margin="0 0 0 10" Name="IdExamination"></TextBox>
            <Button Content="OK" x:Name="ButtonOk" Click="ButtonOk_Click"></Button>
        </StackPanel>
    </Grid>
</Window>
